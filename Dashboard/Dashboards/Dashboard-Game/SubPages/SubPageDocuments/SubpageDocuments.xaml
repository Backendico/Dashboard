                                    <Border  Margin="0,10"    CornerRadius="5" BorderBrush="Gray" BorderThickness="2">
                                        <StackPanel>
                                            <!--link-->
                                            <Border BorderBrush="Gray" BorderThickness="0,0,0,2">
