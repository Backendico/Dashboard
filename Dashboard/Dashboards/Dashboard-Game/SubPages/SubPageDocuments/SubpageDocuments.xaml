                                                    </Border>

                                                    <!--Method-->
                                                    <Border BorderBrush="Gray" >
                                                        <StackPanel  Orientation="Horizontal">
                                                            <!--Token-->
                                                            <Border BorderBrush="Gray" BorderThickness="0,0,2,0">
                                                                <TextBlock Margin="10" TextAlignment="Center" VerticalAlignment="Center" Text="Token"/>
                                                            </Border>

                                                            <!--Username-->
                                                            <Border BorderBrush="Gray" BorderThickness="0,0,2,0">
                                                                <TextBlock Margin="10" TextAlignment="Center" VerticalAlignment="Center" Text="Studio"/>
                                                            </Border>

                                                            <!--Password-->
                                                            <Border BorderBrush="Gray" BorderThickness="0,0,2,0">
                                                                <TextBlock Margin="10" TextAlignment="Center" VerticalAlignment="Center" Text="TokenPlayer"/>
                                                            </Border>

                                                        </StackPanel>
                                                    </Border>
                                                </StackPanel>
                                            </Border>

                                            <!--Response-->
                                            <Border BorderBrush="Gray" BorderThickness="0,0,0,2">
                                                <StackPanel Orientation="Horizontal">
                                                    <!--Method-->
                                                    <Border  Width="120" Background="LightGray"  BorderBrush="Gray" BorderThickness="0,0,2,0" >
                                                        <TextBlock  TextAlignment="Center" Margin="20,10" Text="Response"/>
                                                    </Border>
                                                    <TextBlock Margin="10,0" Text="A Json-type response containing the player settings will be sent to you." VerticalAlignment="Center" />

                                                </StackPanel>
                                            </Border>

                                            <!--Info Tips-->
