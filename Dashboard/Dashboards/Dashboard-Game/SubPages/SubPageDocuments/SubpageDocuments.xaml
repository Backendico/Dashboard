                                                <Run FontWeight="Bold" Text="Tips:"/>
                                                <LineBreak />
                                                <Run Text="&#xE915;" FontFamily="Segoe MDL2 Assets"/> <Run Text="The information received contains TOKENPLAYER, take care of it."/>
                                                 <LineBreak />
                                                <Run Text="&#xE915;" FontFamily="Segoe MDL2 Assets"/> <Run Text="Do not show the user personal information such as: contact number, username, email and token to other players"/>
                                                </TextBlock>
                                            </StackPanel>

                                        </StackPanel>
                                    </Border>

                                    <!--Login username Password-->
                                    <Border CornerRadius="5" BorderBrush="Gray" BorderThickness="2">
                                        <StackPanel>
                                            <!--link-->
                                            <Border BorderBrush="Gray" BorderThickness="0,0,0,2">
                                                <StackPanel Orientation="Horizontal">

                                                    <!--Link-->
                                                    <Border  Width="120" Background="LightGray"  BorderBrush="Gray" CornerRadius="4,0,0,0" BorderThickness="0,0,2,0" >
